{
  "name": "react-ssr",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run dev:server && npm run dev:client",
    "dev:server": "tsx server.ts",
    "dev:client": "npm run build:client && vite --config vite.config.ts dev",
    "build": "rimraf dist && tsc -p tsconfig.json && npm run build:client && npm run build:server && npm run copy-files",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:server": "vite build --ssr src/client/entry-server.tsx --outDir dist/server",
    "build:css": "cp -r src/css dist/css",
    "preview": "vite preview",
    "preview:server": "node server-preview.cjs",
    "serve": "npm run build && cross-env NODE_ENV=production node ./dist/server",
    "copy-files": "copyfiles \"public/**/*\" dist && copyfiles -u 2 \"dist/client/**/*\" dist && copyfiles -u 2 \"dist/client/assets/**/*\" dist/public",
    "generate:readme": "node generate-readme.js",
    "test:client": "lhci autorun"
  },
  "dependencies": {
    "@types/serve-static": "^1.15.4",
    "axios": "^1.3.4",
    "compression": "1.7.4",
    "cross-env": "^7.0.3",
    "express": "4.18.2",
    "serve-static": "^1.15.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.3",
    "react-router": "^6.9.0",
    "react-router-dom": "^6.9.0",
    "dayjs": "~1.11.8",
    "tsx": "^3.13.0"
  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "@types/express": "^4.17.18",
    "@types/compression": "1.7.4",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.1.0",
    "@lhci/cli": "0.12.0",
    "typescript": "^4.9.3",
    "rimraf": "^5.0.5",
    "vite": "^4.1.0",
    "terser": "5.21.0"
  }
}
